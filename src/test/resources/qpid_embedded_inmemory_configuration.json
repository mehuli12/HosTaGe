{
  "name": "Embedded Broker",
  "modelVersion": "8.0",
  "authenticationproviders": [
    {
      "name": "hardcoded",
      "type": "Plain",
      "secureOnlyMechanisms": [],
      "users": [
        {
          "name": "guest",
          "password": "guest",
          "type": "managed"
        }
      ]
    }
  ],
  "ports": [
    {
      "name": "AMQP",
      "port": "${qpid.amqp_port}",
      "bindingAddress" : "127.0.0.1",
      "protocols": [
        "AMQP_0_9", "AMQP_0_9_1", "AMQP_1_0"
      ],
      "authenticationProvider": "hardcoded",
      "virtualhostaliases": [
        {
          "name": "defaultAlias",
          "type": "defaultAlias"
        }
      ]
    }
  ],
  "virtualhostnodes": [
    {
      "name": "default",
      "type": "Memory",
      "defaultVirtualHostNode": "true",
      "virtualHostInitialConfiguration": "{\"type\": \"Memory\"}"
    }
  ]
}